			<if test="clys != null">CLYS = #{clys}, </if>
			<if test="sd != null">SD = #{sd}, </if>
			<if test="ttid != null">TTID = #{ttid}, </if>
			<if test="psdd != null">PSDD = #{psdd}, </if>
			<if test="pssj != null">PSSJ = #{pssj}, </if>
